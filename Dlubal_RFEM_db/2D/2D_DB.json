                        "define_L1_L2": {
                            "label": "Define All Bay Lengths",
                            "active_inputs": [
                                "L_1",
                                "L_2",
                                "n",
                                "H",
                                "H_1"
                            ],
                            "calculated": [
                                "L"
                            ]
                        }
                    }
                },
                "parameters": {
                    "n": {
                        "label": "Number of bays",
                        "type": "int",
                        "default": 8,
                        "constraints": {
                            "min": 2,
                            "max": 50,
                            "step": 2,
                            "must_be_even": true
                        }
                    },
                    "L": {
                        "label": "Total length",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 8,
                        "dependency": "input_via != 'define_L1_L2'"
                    },
                    "L_1": {
                        "label": "Length of outer bays",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 1
                    },
                    "L_2": {
                        "label": "Length of inner bays",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 1
                    },
                    "H": {
                        "label": "Truss height (Depth)",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 1.2,
                        "description": "The constant vertical distance between the top and bottom chord."
                    },
                    "H_1": {
                        "label": "Bottom chord sag",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 1.0,
                        "description": "The vertical distance from the support line to the lowest point of the curve."
                    }
                }
            }
        },
