        {
            "id": "000591",
            "name": "Curved Ridge Beam (2D)",
            "dimensionality": "2D",
            "main_member": "beam",
            "material": "wood",
            "metadata": {
                "description": "A parametric timber or steel ridge beam with a curved bottom chord and optional end cantilevers. Modeled as a 2D surface.",
                "tags": [
                    "beam",
                    "roof",
                    "timber",
                    "glulam",
                    "curved",
                    "cantilever"
                ],
                "classification": {
                    "structure_class": "Surface / Plate",
                    "geometry_source": "Parametric Arc"
                },
                "visual_aids": {
                    "geometry_overview": "",
                    "cantilever_detail": ""
                }
            },
            "inputs": {
                "options": {
                    "wedge_included": {
                        "label": "Include Ridge Wedge",
                        "type": "boolean",
                        "default": false,
                        "description": "Models the extra stiffening wedge at the peak of the beam."
                    }
                },
                "main_geometry": {
                    "L": {
                        "label": "Total Beam Length",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 10
                    },
                    "h": {
                        "label": "Beam Height (Eave)",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 1
                    },
                    "h_ap": {
                        "label": "Beam Height (Ridge)",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 1,
                        "description": "Height at the apex (center) of the beam."
                    },
                    "delta": {
                        "label": "Roof Pitch (Angle)",
                        "type": "float",
                        "unit": "UNIT.ANGLE",
                        "default_expression": "PI / 18",
                        "visual_role": "Slope Angle"
                    },
                    "R": {
                        "label": "Bottom Chord Radius",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 15
                    }
                },
                "cantilever_geometry": {
                    "k": {
                        "label": "Cantilever Length",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 1
                    },
                    "delta_s": {
                        "label": "Cantilever End Angle",
                        "type": "float",
                        "unit": "UNIT.ANGLE",
                        "default_expression": "PI / 18"
                    },
                    "delta_k": {
                        "label": "Cantilever Bottom Angle",
                        "type": "float",
                        "unit": "UNIT.ANGLE",
                        "default_expression": "PI / 18"
                    }
                }
            }
        },
        {
            "id": "001637",
            "name": "Bowstring Truss",
            "dimensionality": "2D",
            "main_member": "truss",
            "material": "steel",
            "metadata": {
                "description": "A 2D truss with a curved top chord and straight bottom chord. Features variable bay lengths defined by three specific input modes.",
                "tags": [
                    "truss",
                    "steel",
                    "roof",
                    "bowstring",
                    "curved"
                ],
                "classification": {
                    "structure_class": "Truss / Lattice",
                    "geometry_source": "Parametric Arc"
                },
                "visual_aids": {
                    "structure_diagram": "",
                    "bay_definition": ""
                }
            },
            "inputs": {
                "topology": {
                    "n": {
                        "label": "Number of bays",
                        "type": "int",
                        "default": 6,
                        "constraints": {
                            "min": 2,
                            "max": 50,
                            "step": 2,
                            "must_be_even": true
                        }
                    }
                },
                "selection_modes": {
                    "parameter_name": "input_via",
                    "label": "Input Definition Mode",
                    "options": {
                        "define_L1": {
                            "label": "Define Total Length & Outer Bays",
                            "required_inputs": [
                                "L",
                                "L_1"
                            ],
                            "calculated_outputs": [
                                "L_2"
                            ]
                        },
                        "define_L2": {
                            "label": "Define Total Length & Inner Bays",
                            "required_inputs": [
                                "L",
                                "L_2"
                            ],
                            "calculated_outputs": [
                                "L_1"
                            ]
                        },
                        "define_L1_L2": {
                            "label": "Define All Bay Lengths",
                            "required_inputs": [
                                "L_1",
                                "L_2"
                            ],
                            "calculated_outputs": [
                                "L"
                            ]
                        }
                    }
                },
                "geometry": {
                    "H": {
                        "label": "Truss Height",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 3
                    },
                    "L": {
                        "label": "Total Length",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 12,
                        "dependency": "input_via != 'define_L1_L2'"
                    },
                    "L_1": {
                        "label": "Length of Outer Bays",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 2,
                        "dependency": "input_via != 'define_L2'"
                    },
                    "L_2": {
                        "label": "Length of Inner Bays",
                        "type": "float",
                        "unit": "UNIT.LENGTH",
                        "default": 2,
                        "dependency": "input_via != 'define_L1'"
                    }
                }
            }
        },
        {
            "id": "001671",
